
# Calculation

## Priority
PRINT 3 + 5 * 2 - 4 / 3 + 3 ;
= 14.6667
PRINT 3 + 5 * 2 - 4 / 3 + 3 > 300 ;
= FALSE

## With Variable
MAKE "A 200
MAKE "B 100
MAKE "B :B + 50.5 * 2
PRINT THING "A + 50 / 50 = :B ;
= TRUE

## With Parentheses
PRINT 5 * ( 3 + 3 ) ;
= 30
PRINT ( 2 + 3 ) * ( ( 4 + 5 ) - 8 ) ;
= 5

# Variable

MAKE "A 100 ;
MAKE "B :A + 100 ;
PRINT :B ;
= 200
MAKE "C "miao
PRINT THING "C ;
= miao

# Bool
PRINT AND 100 > 99 70 + 30 = 100 ;
= TRUE
PRINT AND 100 > 99 70 + 30 < 100 ;
= FALSE
PRINT OR 100 < 99.5 3 = 2 + 2 / 2 ;
= TRUE
MAKE "A 100 + 3
PRINT NOT THING "A = 103 ;
= FALSE

# READLIST
MAKE "A READLIST [ 1 2 3 "four ] PRINT :A ;
= [ 1 2 3 four ]

# IF
IF 2 < 3 [ PRINT "Right. ] [ PRINT "Wrong. ] ;
= Right.
IF 4 * 4 < 3 * 3 [ MAKE "ret "Wrong. ] [ MAKE "ret "Right. ] PRINT :ret ;
= Right.

# REPEAT
PRINT "Before REPEAT 3 [ PRINT "Repeating(3)... WAIT 1000000 ] PRINT "After ;

# NUMBERP
MAKE "A 100.1 PRINT NUMBERP :A ;
= TRUE
PRINT NUMBERP "hello ;
= FALSE

# WORDP
PRINT WORDP "miao ;
= TRUE
PRINT WORDP 333.33 ;
= FALSE

# WORD
MAKE "A "miao MAKE "B "mew PRINT WORD :A :B ;
= miaomew





